<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	.gwt-Label {
		color: Black;
		font-stretch: normal;
		font-style: normal;
		background-color: White;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:AbsolutePanel height="200px" width="762px">
			<g:at left="394" top="167">
				<g:Button styleName="{style.important}" ui:field="button" text="Giriş" />
			</g:at>
			<g:at left="339" top="31">
				<g:Label text="Kullanıcı Girişi" styleName="{style.important}" width="151px" height="17px" wordWrap="false"/>
			</g:at>
			<g:at left="339" top="64">
				<g:TextBox ui:field="username"/>
			</g:at>
			<g:at left="339" top="103">
				<g:PasswordTextBox ui:field="password"/>
			</g:at>
			<g:at left="339" top="140">
				<g:CheckBox text="Beni Hatırla"/>
			</g:at>
			<g:at left="223" top="70">
				<g:Label text="Kullanıcı Adı:"/>
			</g:at>
			<g:at left="273" top="109">
				<g:Label text="Şifre:"/>
			</g:at>
		</g:AbsolutePanel>
	</g:HTMLPanel>
</ui:UiBinder> 